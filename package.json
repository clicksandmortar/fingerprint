{
  "name": "fingerprint",
  "version": "1.0.0",
  "description": "Clicks &amp; Mortar Fingerprint Tracking &amp; Triggers",
  "author": "clicksandmortar",
  "license": "MIT",
  "repository": "clicksandmortar/fingerprint",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "targets": {
    "legacy": {
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "outputFormat": "global",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "build:fingerprint": "parcel build src/fingerprint.tsx --target legacy",
    "watch:fingerprint": "parcel watch src/fingerprint.tsx --target legacy",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": ">18.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "@parcel/packager-ts": "2.9.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/js-cookie": "^3.0.3",
    "@types/mixpanel-browser": "^2.47.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.9.3",
    "parcel-transformer-env-variables-injection": "^0.1.2",
    "prettier": "^2.0.4",
    "react": ">16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "@fingerprintjs/fingerprintjs-pro-react": "^2.5.0",
    "@sentry/browser": "^7.61.0",
    "@sentry/cli": "^2.20.4",
    "@sentry/react": "^7.61.0",
    "@tanstack/react-query": "^4.32.0",
    "axios": "^1.4.0",
    "js-cookie": "^3.0.5",
    "mixpanel-browser": "^2.47.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.45.4",
    "react-idle-timer": "^5.7.2",
    "use-exit-intent": "^1.0.7",
    "use-local-storage-state": "^18.3.3",
    "uuid": "^9.0.1"
  },
  "@parcel/transformer-js": {
    "inlineEnvironment": [
      "MIXPANEL_TOKEN"
    ]
  }
}
